{
    "program": {
        "binary": "bin/omaha_client_service"
    },
    "sandbox": {
        "features": [
            "build-info",
            "root-ssl-certificates"
        ],
        "services": [
            "fuchsia.cobalt.LoggerFactory",
            "fuchsia.logger.LogSink",
            "fuchsia.net.NameLookup",
            "fuchsia.net.SocketProvider",
            "fuchsia.posix.socket.Provider",
            "fuchsia.stash.Store",
            "fuchsia.sys.Launcher"
        ]
    }
}
