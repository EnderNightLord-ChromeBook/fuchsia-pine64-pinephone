// Component manifest for the `echo_server` example program, which hosts the Echo service used
// by `echo_client`.
{
    "program": {
        "binary": "bin/echo_server",
    },
    "expose": [
        {
            "legacy_service": "/svc/fidl.examples.routing.echo.Echo",
            "from": "self",
        },
        {
            "directory": "/hub",
            "from": "framework",
        },
    ],
}
