// Realm for integration test that creates and binds to component instances in a collection.
{
    "program": {
        "binary": "bin/collection_realm",
    },
    "use": [
        {
            "legacy_service": "/svc/fuchsia.logger.LogSink",
        },
        {
            "legacy_service": "/svc/fuchsia.sys2.Realm",
            "from": "framework",
        },
    ],
    // Route Echo service from server to collection.
    "offer": [
        {
            "legacy_service": "/svc/fuchsia.logger.LogSink",
            "from": "realm",
            "to": [
                {
                    "dest": "#coll",
                },
            ],
        },
        {
            "legacy_service": "/svc/fidl.examples.routing.echo.Echo",
            "from": "#echo_server",
            "to": [
                {
                    "dest": "#coll",
                },
            ],
        },
    ],
    "children": [
        {
            "name": "echo_server",
            "url": "fuchsia-pkg://fuchsia.com/collections_integration_test#meta/echo_server.cm",
        },
    ],
    "collections": [
        {
            "name": "coll",
            "durability": "transient",
        },
    ],
}
