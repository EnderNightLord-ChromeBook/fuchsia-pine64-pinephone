library zz;

[Transport="Syscall"]
protocol Cprng {
    [Internal]
    DrawOnce() -> (status status, vector<byte> buffer);

    /// Draw from the kernel's CPRNG.
    [Vdsocall,
     In0="mutable<vector<byte>>"]
    Draw(vector<byte> buffer) -> ();

    /// Add entropy to the kernel CPRNG.
    AddEntropy(vector<byte> buffer) -> (status status);
};
