library zz;

[Transport="Syscall"]
protocol Mtrace {
    [In0="handle<resource>:kind=ROOT",
     In4="mutable<vector<byte>>"]
    Control(handle<resource> handle,
            uint32 kind,
            uint32 action,
            uint32 options,
            vector<byte> ptr) ->
        (status status);
};
