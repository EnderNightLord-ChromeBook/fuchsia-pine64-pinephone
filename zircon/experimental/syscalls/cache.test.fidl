library zz;

[Transport="Syscall"]
protocol Cache {
    // TODO(scottmg): Something to tell In0 to be a void* in C.
    /// Flush CPU data and/or instruction caches.
    [Vdsocall]
    Flush(vaddr addr, usize size, uint32 options) -> (status status);
};
